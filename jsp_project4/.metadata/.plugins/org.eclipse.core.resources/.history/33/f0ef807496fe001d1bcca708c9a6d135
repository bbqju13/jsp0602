<mapper namespace="UserMapper">
	<!-- id pw birth age name -->
	<insert id="reg" parameterType="uvo">
		insert into user(id, pw, birth,
		age, name)
		values(#{id}, #{pw}, #{birth}, #{age}, #{name})
	</insert>
</mapper>
